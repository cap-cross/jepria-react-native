{
  "expo": {
    "name": "jepria-react-native",
    "description": "Jepria UI implemented on React Native",
    "slug": "jepria-react-native",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "loading": {
      "icon": "./assets/images/icon.png"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "org.jepria.feature.demo"
    },
    "android": {
      "package": "org.jepria.feature.demo"
    },    
    "androidStatusBar": {
      "backgroundColor": "#289592"
    }
  }
}
